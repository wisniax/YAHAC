<UserControl x:Class="YAHAC.MVVM.Model.MinecraftItemBox"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:local="clr-namespace:YAHAC.MVVM.Model"
			 mc:Ignorable="d" 
			 d:DesignHeight="34" d:DesignWidth="34"
			 Background="Transparent"
			 BorderThickness="0" 
			 Width="34" Height="34" 
			 HorizontalAlignment="Left" 
			 VerticalAlignment="Top"
			 MaxHeight="34" MaxWidth="34"
			 MinHeight="34" MinWidth="34"
			 Margin="0,2,0,2">
	<Border BorderThickness="1"
			Background="Transparent"
			BorderBrush="DarkOrange"
			CornerRadius="2">
		<Border Background="Transparent">
			<Border.Clip>
				<RectangleGeometry RadiusX="1"
							   RadiusY="1"
							   Rect="0,0,32,32"/>
			</Border.Clip>
			<!--<TextBox Text="CHUJ" 
					 FontSize="10" 
					 Height="20" Width="30" 
					 BorderThickness="0"
					 Background="Transparent"
					 Foreground="Wheat"
					 HorizontalContentAlignment="Center"
					 VerticalContentAlignment="Center"/>-->
			<Image Source="/Resources/smth.bmp"
				   Name="ImageBox">
			</Image>
		</Border>
	</Border>
</UserControl>
